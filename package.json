{
  "name": "react-typescript",
  "version": "1.0.0",
  "description": "Typescript, React, Webpack, Express App from scratch",
  "main": "server.js",
  "scripts": {
    "build:typescript": "tsc",
    "build": "webpack --config webpack.dev.js",
    "build:production": "webpack --config webpack.prod.js",
    "dev": "ts-node --inspect=5858 ./src/server/server.ts",
    "nodemon": "nodemon --inspect ./dist/server.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "clean": "rimraf ./node_modules/",
    "test": "rimraf ./test_build/ && tsc -p tsconfig-test.json && alsatian \"./test_build/test/**/*.test.js\"",
    "test-test": "alsatian \"./test_build/test/**/*.test.js\"",
    "lint": "tslint -c tslint.json \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanmynah/react-typescript.git"
  },
  "author": "mrybintsev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alanmynah/react-typescript/issues"
  },
  "homepage": "https://github.com/alanmynah/react-typescript#readme",
  "dependencies": {
    "@types/azure": "^0.9.19",
    "@types/express": "^4.16.0",
    "@types/uuid": "^3.4.3",
    "@types/yargs": "^11.0.0",
    "axios": "^0.18.0",
    "azure-storage": "^2.8.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "lodash": "^4.17.10",
    "npm": "^6.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "semantic-ui-react": "^0.81.1",
    "ts-node": "^6.1.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.6",
    "alsatian": "^2.2.1",
    "awesome-typescript-loader": "^5.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "v4.0.0-alpha.0",
    "html-webpack-plugin": "^3.2.0",
    "mock-fetch-api": "^1.0.7",
    "neworbit-tslint-config": "^2.0.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "tslint": "^5.9.1",
    "typescript": "^2.9.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-merge": "^4.1.2"
  }
}
